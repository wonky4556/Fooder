# Google OAuth (from console.cloud.google.com)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Comma-separated SHA-256 hashes of admin emails
# Generate: echo -n "admin@example.com" | tr '[:upper:]' '[:lower:]' | tr -d ' ' | shasum -a 256 | cut -d' ' -f1
ADMIN_EMAIL_HASHES=

# Cognito (from AppStack CloudFormation outputs after first deploy)
VITE_COGNITO_USER_POOL_ID=us-east-1_XXXXXXXXX
VITE_COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
VITE_COGNITO_DOMAIN=fooder-dev.auth.us-east-1.amazoncognito.com

# API URL (empty for local dev â€” Vite proxy handles /api; set to deployed URL for prod)
VITE_API_URL=

# Cognito redirect URLs (defaults work for local dev)
VITE_COGNITO_REDIRECT_SIGN_IN=http://localhost:5173/auth/callback
VITE_COGNITO_REDIRECT_SIGN_OUT=http://localhost:5173/login

# DynamoDB table name (from CloudFormation outputs or SSM: /{stage}/fooder/tables/users/name)
USERS_TABLE_NAME=Fooder-Dev-InfraStack-UsersTableXXXXXX

# KMS (for local Lambda testing)
PII_KEY_ARN=arn:aws:kms:us-east-1:123456789012:key/your-key-id
